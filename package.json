{
    "name": "verus-warehouse-api",
    "version": "1.0.0",
    "description": "Verus Warehouse Management System",
    "private": true,
    "type": "module",
    "engines": {
        "node": "^20.19.0 || >=22.12.0"
    },
    "scripts": {
        "dev": "vite",
        "build": "run-p type-check \"build-only {@}\" --",
        "build-only": "vite build",
        "type-check": "vue-tsc --build --force",
        "preview": "vite preview",
        "lint": "eslint resources/js --ext .ts,.vue --fix",
        "format": "prettier --write resources/js/"
    },
    "dependencies": {
        "@mdi/font": "^7.4.47",
        "pinia": "^3.0.3",
        "vue": "^3.5.18",
        "vue-router": "^4.5.1",
        "vuetify": "^3.10.0"
    },
    "devDependencies": {
        "@tsconfig/node22": "^22.0.2",
        "@types/node": "^22.16.5",
        "@vitejs/plugin-vue": "^6.0.1",
        "@vue/eslint-config-prettier": "^10.2.0",
        "@vue/eslint-config-typescript": "^14.6.0",
        "@vue/tsconfig": "^0.7.0",
        "axios": "^1.6.4",
        "eslint": "^9.31.0",
        "eslint-plugin-vue": "~10.3.0",
        "laravel-vite-plugin": "^2.0.0",
        "npm-run-all2": "^8.0.4",
        "prettier": "3.6.2",
        "sass": "^1.92.1",
        "typescript": "~5.8.0",
        "vite": "^7.0.6",
        "vite-plugin-vue-devtools": "^8.0.0",
        "vite-plugin-vuetify": "^2.1.2",
        "vue-tsc": "^3.0.4"
    },
    "keywords": ["laravel", "vue", "warehouse", "management"],
    "author": "Verus Team",
    "license": "MIT"
}
